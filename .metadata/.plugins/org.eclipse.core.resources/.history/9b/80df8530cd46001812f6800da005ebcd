public class FindHalfTimesElement {

	
	public static int findHalfTimesElement(int []nums) {
		Arrays.sort(nums);
		//数组一半的长度
		int max=nums.length/2;
		int times=0;
		int l=0, r=1; 
		while(r<nums.length) {
			if(nums[l]==nums[r]) {
				times++;
			}else {
				
			}
			
		}
		return 0;
	}
	
