		return null;
	}
	
	/**
	 * 
	 * @param array
	 * @param start
	 * @param end
	 * @return
	 */
	public static int findIndexInArray(int []array, int data, int start, int end) {
		for(int i=start; i<=end; i++) {
			if(array[i]==data) {
				return i;
			}
		}
		return -1;
	}
	
