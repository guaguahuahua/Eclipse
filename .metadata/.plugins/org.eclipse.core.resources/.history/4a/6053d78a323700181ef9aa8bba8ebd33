package com.xjtu.algorithm;

public class BinSearch {
	
	public static int binsearch(int []nums, int find) {
		//处理一下数组为空的情形
		if(nums.length==0) {
			return -1;
		}
		int left=0;
		int right=nums.length-1;
		while(left<right) {
			int mid=left+(right-left)/2;
			if(nums[mid]==find) {
				return mid;
			}else if(nums[mid]) {
				
			}
		}
		
		return -1;
	}
	
	public static void main(String []args) {
		int []nums= {0,1,4,8,14,19};
	}

}
