	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Node head=new Node('a');
		Node n1=new Node('b');
		Node n2=new Node('c');
		Node n3=new Node('d');
		Node n4=new Node('e');
		
		head.left=n1;
		n1.left=n2;
		n2.left=n3;
		n3.left=n4;
		
		head.right=n2;
		n1.right=n4;
		n2.right=null;
		n3.right=n1;
		n4.right=null;
		
		
	}

